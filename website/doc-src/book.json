{
    "title": "Sutra CMS",
    "Description": "Sutra CMS developer documentation",
    "root": "./",
    "plugins": [ "-sharing", "versions", "todo", "github", "prism", "-highlight", "youtube", "simple-page-toc",
          "theme-vuejs@git+https://github.com/pearofducks/gitbook-plugin-theme-vuejs.git", "-fontsettings" ],
    "pluginsConfig": {
        "github": {
            "url": "https://datamosaic.github.io/sutra-cms/"
